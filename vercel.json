    },
    {
      "source": "/eletricista-freixo-espada-cinta",
      "destination": "/eletricista-freixo-espada-cinta.html"
    },
    {
      "source": "/electricista-vila-real",
      "destination": "/eletricista-vila-real.html"
    },
    {
      "source": "/electricista-braganca",
      "destination": "/eletricista-braganca.html"
    },
    {
      "source": "/electricista-chaves",
